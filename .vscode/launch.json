{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "windows": { "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd" },
            "runtimeArgs" : [],
            "program": "${workspaceFolder}/app/main.js",
            "args": ["--", "headless=true", "manifest=d:/_Code/Main/glTF-Asset-Generator/Output/Positive/Manifest.json", "outputDirectory=d:/_Code/Main/glTF-Asset-Generator/tempImages/Figures/SampleImages"],
            "cwd": "${workspaceFolder}",
            "outputCapture": "std",
        },
        {
            "name": "Debug Renderer",
            "type": "chrome",
            "request": "launch",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "windows": { "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd" },
            "runtimeArgs" : ["${workspaceFolder}/app/main.js", "--", "headless=true", "manifest=d:/_Code/Main/glTF-Asset-Generator/Output/Positive/Manifest.json", "outputDirectory=d:/_Code/Main/glTF-Asset-Generator/tempImages/Figures/SampleImages"],
            "webRoot": "${workspaceFolder}/app",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Debug Renderer V2",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "windows": { "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd" },
            "runtimeArgs" : ["--debug=5858"],
            "program": "${workspaceFolder}/app/main.js",
            "args": ["--", "headless=true", "manifest=d:/_Code/Main/glTF-Asset-Generator/Output/Positive/Manifest.json", "outputDirectory=d:/_Code/Main/glTF-Asset-Generator/tempImages/Figures/SampleImages"],
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": null
        },
        {
            "name": "Debug Main Process",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "windows": {
              "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
            },
            "args" : ["."],
            "outputCapture": "std",
          }
    ]
}